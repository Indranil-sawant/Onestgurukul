            "content": "<!DOCTYPE html>\n<html lang=\"en\">\n\n<head>\n  <meta charset=\"utf-8\">\n  <meta content=\"width=device-width, initial-scale=1.0\" name=\"viewport\">\n  <title>Admissions - College Bootstrap Template</title>\n  <meta name=\"description\" content=\"\">\n  <meta name=\"keywords\" content=\"\">\n\n  <!-- Favicons -->\n  <link href=\"assets/img/favicon.png\" rel=\"icon\">\n  <link href=\"assets/img/apple-touch-icon.png\" rel=\"apple-touch-icon\">\n\n  <!-- Fonts -->\n  <link href=\"https://fonts.googleapis.com\" rel=\"preconnect\">\n  <link href=\"https://fonts.gstatic.com\" rel=\"preconnect\" crossorigin>\n  <link href=\"https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&family=Raleway:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap\" rel=\"stylesheet\">\n\n  <!-- Vendor CSS Files -->\n  <link href=\"assets/vendor/bootstrap/css/bootstrap.min.css\" rel=\"stylesheet\">\n  <link href=\"assets/vendor/bootstrap-icons/bootstrap-icons.css\" rel=\"stylesheet\">\n  <link href=\"assets/vendor/aos/aos.css\" rel=\"stylesheet\">\n  <link href=\"assets/vendor/swiper/swiper-bundle.min.css\" rel=\"stylesheet\">\n  <link href=\"assets/vendor/glightbox/css/glightbox.min.css\" rel=\"stylesheet\">\n\n  <!-- Main CSS File -->\n  <link href=\"assets/css/main.css\" rel=\"stylesheet\">\n\n</head>\n\n<body class=\"admissions-page\">\n\n  <header id=\"header\" class=\"header d-flex align-items-center sticky-top\">\n    <div class=\"header-container container-fluid container-xl position-relative d-flex align-items-center justify-content-end\">\n\n      <a href=\"index.html\" class=\"logo d-flex align-items-center me-auto\">\n        <img src=\"assets/img/onest.svg\" alt=\"\"> \n      </a>\n\n       <nav id=\"navmenu\" class=\"navmenu\">\n        <ul>\n          <li><a href=\"index.html\" >Home</a></li>\n          <li class=\"dropdown\"><a href=\"about.html\"><span>About Us</span></a>\n          </li>\n\n          <li><a href=\"students-life.html\">Curriculum</a></li>\n          <li><a href=\"campus-facilities.html\">Campus &amp; Facilities</a></li>\n          <li><a href=\"admissions.html\">Admissions</a></li>\n          <li><a href=\"contact.html\">Contact</a></li>\n        </ul>\n        <i class=\"mobile-nav-toggle d-xl-none bi bi-list\"></i>\n      </nav>\n    </div>\n  </header>\n\n  <main class=\"main\">\n\n    <!-- Page Title -->\n    <!-- End Page Title -->\n\n    <!-- Admissions Section -->\n    <section id=\"admissions\" class=\"admissions section\">\n\n      <div class=\"container\" data-aos=\"fade-up\" data-aos-delay=\"100\">\n\n        <div class=\"row gy-4\">\n          <div class=\"col-lg-6\" data-aos=\"fade-up\" data-aos-delay=\"200\">\n            <div class=\"application-steps\">\n              <h3 class=\"section-subtitle\">How to Apply</h3>\n              <div class=\"steps-wrapper\">\n                <div class=\"step-item\">\n                  <div class=\"step-number\">01</div>\n                  <div class=\"step-content\">\n                    <h4>Visiting Campus</h4>\n                    <p>Collect prospectus from admin office.</p>\n                  </div>\n                </div>\n                <div class=\"step-item\">\n                  <div class=\"step-number\">02</div>\n                  <div class=\"step-content\">\n                    <h4>Form Submission</h4>\n                    <p>Fill and submit with documents.</p>\n                  </div>\n                </div>\n                <div class=\"step-item\">\n                  <div class=\"step-number\">03</div>\n                  <div class=\"step-content\">\n                    <h4>Interaction/Test</h4>\n                    <p>Informal interaction.</p>\n                  </div>\n                </div>\n                <div class=\"step-item\">\n                  <div class=\"step-number\">04</div>\n                  <div class=\"step-content\">\n                    <h4>Fee Payment</h4>\n                    <p>Within 7 days of approval.</p>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n\n          <div class=\"col-lg-6\" data-aos=\"fade-up\" data-aos-delay=\"300\">\n            <div class=\"requirements-card\">\n              <h3 class=\"section-subtitle\">Admission Requirements</h3>\n              <ul class=\"requirements-list\">\n                <li>\n                  <i class=\"bi bi-check-circle\"></i>\n                  <span>Birth Certificate (Municipal/Aadhaar linked)</span>\n                </li>\n                <li>\n                  <i class=\"bi bi-check-circle\"></i>\n                  <span>Aadhaar Card (Student + Parents)</span>\n                </li>\n                <li>\n                  <i class=\"bi bi-check-circle\"></i>\n                  <span>Address Proof (Electricity bill/Rent agreement)</span>\n                </li>\n                <li>\n                  <i class=\"bi bi-check-circle\"></i>\n                  <span>Caste Certificate (if applicable)</span>\n                </li>\n                <li>\n                  <i class=\"bi bi-check-circle\"></i>\n                  <span>Passport-size photos (Student: 4, Parents: 2)</span>\n                </li>\n                <li>\n                  <i class=\"bi bi-check-circle\"></i>\n                  <span>Medical Certificate (if applicable)</span>\n                </li>\n              </ul>\n              <div class=\"special-note\">\n                <i class=\"bi bi-info-circle\"></i>\n                <p>All original documents must be submitted for verification at the time of admission</p>\n              </div>\n            </div>\n          </div>\n        </div>\n\n        <div class=\"row mt-5 gy-4\">\n           <div class=\"col-lg-6\" data-aos=\"fade-up\" data-aos-delay=\"200\">\n            <div class=\"application-steps\">\n              <h3 class=\"section-subtitle\">How to Apply Online</h3>\n              <div class=\"steps-wrapper\">\n                <div class=\"step-item\">\n                  <div class=\"step-number\">01</div>\n                  <div class=\"step-content\">\n                    <h4>Registration</h4>\n                    <p>Create an account on our admissions portal.</p>\n                  </div>\n                </div>\n                <div class=\"step-item\">\n                  <div class=\"step-number\">02</div>\n                  <div class=\"step-content\">\n                    <h4>Fill Application Form</h4>\n                    <p>Enter student and parent details carefully.</p>\n                  </div>\n                </div>\n                <div class=\"step-item\">\n                  <div class=\"step-number\">03</div>\n                  <div class=\"step-content\">\n                    <h4>Upload Documents</h4>\n                    <p>Submit birth certificate, ID proof & recent photo.</p>\n                  </div>\n                </div>\n                <div class=\"step-item\">\n                  <div class=\"step-number\">04</div>\n                  <div class=\"step-content\">\n                    <h4>Submit & Confirmation</h4>\n                    <p>Review, submit form, and receive confirmation mail/SMS.</p>\n                  </div>\n                </div>\n                \n              </div>\n              <a href=\"mailto:admissions@yourdomain.com?subject=Admission%20Enquiry%20-%20[Child's%20Name]%20for%20[Class]%20&body=Dear%20Admissions%20Team,%0D%0A%0D%0AI%20would%20like%20to%20apply%20for%20admission%20for%20my%20child.%0D%0A%0D%0AChild's%20Name:%20%0D%0AAge:%20%0D%0AClass%20Seeking%20Admission%20to:%20%0D%0AParent's%20Name:%20%0D%0AContact%20Number:%20%0D%0AEmail:%20%0D%0A%0D%0AThank%20you.\" \n                class=\"apply-btn\">\n                      Start Application\n              </a>\n\n            </div>\n            \n          </div>\n\n          <div class=\"col-lg-6\" data-aos=\"fade-up\" data-aos-delay=\"300\">\n            <div class=\"contact-form-card\">\n              <h3 class=\"section-subtitle\">Request Information</h3>\n              <form action=\"https://formspree.io/f/xvgqnaol\" class=\"php-email-form\">\n                <div class=\"row g-3\">\n                  <div class=\"col-12\">\n                    <input type=\"text\" name=\"name\" class=\"form-control\" placeholder=\"Name*\" required=\"\">\n                  </div>\n                  <div class=\"col-md-6\">\n                    <input type=\"email\" name=\"email\" class=\"form-control\" placeholder=\"Email Address*\" required=\"\">\n                  </div>\n                  <div class=\"col-md-6\">\n                    <input type=\"tel\" name=\"phone\" class=\"form-control\" placeholder=\"Phone Number\">\n                  </div>\n                  <div class=\"col-12\">\n                    <select name=\"subject\" class=\"form-select\" required=\"\">\n                      <option selected=\"\" disabled=\"\">Class</option>\n                      <option>Pre-Primary</option>\n                      <option>Grade 1</option>\n                      <option>Grade 2</option>\n                      <option>Grade 3</option>\n                      <option>Grade 4</option>\n                    </select>\n                  </div>\n                  <div class=\"col-12\">\n                    <textarea name=\"message\" class=\"form-control\" rows=\"7\" placeholder=\"Questions or Comments\"></textarea>\n                  </div>\n                  <div class=\"col-12\">\n                    <div class=\"loading\">Loading</div>\n                    <div class=\"error-message\"></div>\n                    <div class=\"sent-message\">Your message has been sent. Thank you!</div>\n                    <button type=\"submit\" class=\"btn btn-request\">Request Information</button>\n                  </div>\n                </div>\n              </form>\n            </div>\n          </div>\n        </div>\n\n        <div class=\"row mt-5\">\n          <div class=\"col-12\" data-aos=\"fade-up\" data-aos-delay=\"100\">\n            <div class=\"deadlines-card\">\n              <h3 class=\"section-subtitle\">Important Deadlines</h3>\n              <div class=\"deadlines-grid\">\n                <div class=\"deadline-item\" data-aos=\"zoom-in\" data-aos-delay=\"200\">\n                  <div class=\"deadline-date\">December-January</div>\n                  <h4>Notification & Application Phase</h4>\n                  <p>For students committed to attending if accepted</p>\n                </div>\n                <div class=\"deadline-item\" data-aos=\"zoom-in\" data-aos-delay=\"300\">\n                  <div class=\"deadline-date\">February 1st-15th</div>\n                  <h4>Form Submission</h4>\n                  <p>Main application deadline for Fall admission</p>\n                </div>\n                <div class=\"deadline-item\" data-aos=\"zoom-in\" data-aos-delay=\"400\">\n                  <div class=\"deadline-date\">March 6th-10th</div>\n                  <h4> Document Verification & Fee Payment</h4>\n                  <p>Priority deadline for scholarship consideration</p>\n                </div>\n                <div class=\"deadline-item\" data-aos=\"zoom-in\" data-aos-delay=\"500\">\n                  <div class=\"deadline-date\">April 1st</div>\n                  <h4>Orientation & Academic Start</h4>\n                  <p>Deadline to confirm attendance and secure your spot</p>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n\n        <div class=\"row mt-5 campus-visit\">\n          <div class=\"col-lg-6\" data-aos=\"fade-right\" data-aos-delay=\"100\">\n            <div class=\"visit-image\">\n              <img src=\"assets/img/education/campus-5.webp\" class=\"img-fluid\" alt=\"Campus Visit\" loading=\"lazy\">\n              <div class=\"image-caption\">Experience our beautiful campus in person</div>\n            </div>\n          </div>\n          <div class=\"col-lg-6\" data-aos=\"fade-left\" data-aos-delay=\"200\">\n            <div class=\"visit-content\">\n              <h3>Schedule a Campus Visit</h3>\n              <p>Our guided campus tours, led by student ambassadors, offer a firsthand glimpse into daily life at our Gurukul. Explore modern smart classrooms, serene meditation spaces, and lively activity centers where holistic learning comes alive. Witness values-based education in action through interactive demonstrations, and meet our passionate educators who nurture both academic excellence and character development. Experience our unique \"Wisdom Walks\" through heritage gardens, and observe how ancient Vedic principles blend seamlessly with 21st-century skills in our maker labs.</p>\n              <ul class=\"visit-options\">\n                <li><i class=\"bi bi-calendar-check\"></i> Daily campus tours (Mon-Fri, 10am &amp; 2pm)</li>\n                <li><i class=\"bi bi-people\"></i> Information sessions with admissions staff</li>\n                <li><i class=\"bi bi-building\"></i> Residence hall viewings</li>\n                <li><i class=\"bi bi-mortarboard\"></i> Faculty meetings (by appointment)</li>\n              </ul>\n              <a href=\"#\" class=\"btn btn-schedule\">Schedule Your Visit</a>\n              <div class=\"virtual-option\">\n                <span>Can't visit in person?</span>\n                <a href=\"#\" class=\"virtual-link\">Take a Virtual Tour <i class=\"bi bi-arrow-right\"></i></a>\n              </div>\n            </div>\n          </div>\n        </div>\n\n      </div>\n\n    </section><!-- /Admissions Section -->\n\n  </main>\n\n  <footer id=\"footer\" class=\"footer position-relative light-background\">\n\n    <div class=\"container footer-top\">\n      <div class=\"row gy-4\">\n        <div class=\"col-lg-4 col-md-6 footer-about\">\n          <a href=\"index.html\" class=\"logo d-flex align-items-center\">\n            <span class=\"sitename\">College</span>\n          </a>\n          <div class=\"footer-contact pt-3\">\n            <p>A108 Adam Street</p>\n            <p>New York, NY 535022</p>\n            <p class=\"mt-3\"><strong>Phone:</strong> <span>+1 5589 55488 55</span></p>\n            <p><strong>Email:</strong> <span>info@example.com</span></p>\n          </div>\n          <div class=\"social-links d-flex mt-4\">\n            <a href=\"\"><i class=\"bi bi-twitter-x\"></i></a>\n            <a href=\"\"><i class=\"bi bi-facebook\"></i></a>\n            <a href=\"\"><i class=\"bi bi-instagram\"></i></a>\n            <a href=\"\"><i class=\"bi bi-linkedin\"></i></a>\n          </div>\n        </div>\n\n        <div class=\"col-lg-2 col-md-3 footer-links\">\n          <h4>Useful Links</h4>\n          <ul>\n            <li><a href=\"#\">Home</a></li>\n            <li><a href=\"#\">About us</a></li>\n            <li><a href=\"#\">Services</a></li>\n            <li><a href=\"#\">Terms of service</a></li>\n            <li><a href=\"#\">Privacy policy</a></li>\n          </ul>\n        </div>\n\n        <div class=\"col-lg-2 col-md-3 footer-links\">\n          <h4>Our Services</h4>\n          <ul>\n            <li><a href=\"#\">Web Design</a></li>\n            <li><a href=\"#\">Web Development</a></li>\n            <li><a href=\"#\">Product Management</a></li>\n            <li><a href=\"#\">Marketing</a></li>\n            <li><a href=\"#\">Graphic Design</a></li>\n          </ul>\n        </div>\n\n        <div class=\"col-lg-2 col-md-3 footer-links\">\n          <h4>Hic solutasetp</h4>\n          <ul>\n            <li><a href=\"#\">Molestiae accusamus iure</a></li>\n            <li><a href=\"#\">Excepturi dignissimos</a></li>\n      